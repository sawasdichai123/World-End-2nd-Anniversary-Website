{
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "api/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "crons": [
        {
            "path": "/api/update-live-status",
            "schedule": "* * * * *"
        },
        {
            "path": "/api/member-videos",
            "schedule": "0 0 * * *"
        }
    ]
}